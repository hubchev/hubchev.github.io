@Article{Feenstra2000Aggregation,
  author    = {{Robert C.} Feenstra and {Gordon H.} Hanson},
  title     = {Aggregation Bias in the Factor Content of Trade: Evidence From {U.S.} Manufacturing},
  journal   = {American Economic Review},
  year      = {2000},
  volume    = {90},
  number    = {2},
  pages     = {155-160},
  abstract  = {In this article, the authors reexamine the finding that the factor content of trade is 'small' and argue that this might be due to aggregation bias. The direct factor requirements can quite easily be computed at a disaggregated industry level, such as the four-digit standard industrial classification (SIC), as in Katz and Murphy (1992). This is not true, however, for the indirect factor requirements, which require the use of an input-output matrix for the economy. The authors attempt to show that the difference between the domestic output and net exports weighting scheme leads to a systematic bias in the factor content of trade. Furthermore, the sign of this bias is the same as that obtained from a regression of net exports on factor contents, similar to that performed by Robert Baldwin (1971). The authors apply this expression of the aggregation bias to the measurement of the factor-content trade in US manufacturing.},
  file      = {Feenstra2000Aggregation.pdf:Feenstra2000Aggregation.pdf:PDF},
  keywords  = {International trade, MANUFACTURING industries, ECONOMICS, UNITED States},
  owner     = {rwl518},
  timestamp = {2015.10.16},
}

